<%- include("header"); -%>
    <div class="box" id="heading">
        <h1> <%= listTitle %> </h1>
    </div>
    <div class="box">
        <% for (var i=0;i < toDoExtra.length;i++){ %>
            <div class="item">
                <input type="checkbox">
                <p><%=toDoExtra[i]%></p><br>
            </div>
        <% } %>
        <form class="item" action="/" method="post">
            <input type="text" name="toDo" placeholder="New Item" autocomplete="off">
            <button type="submit" name="button" value=<%= listTitle %>>+</button>
        </form>
    </div>
<%- include("footer"); -%>